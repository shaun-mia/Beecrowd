#include <stdio.h>

int main() {
    double x, sum = 0;
    int pos = 0;
    for (int i = 0; i < 6; i++) {
        scanf("%lf", &x);
        if (x > 0) {
            pos++;
            sum += x;
        }
    }
    printf("%d valores positivos\n", pos);
    printf("%.1f\n", sum / pos);
    return 0;
}